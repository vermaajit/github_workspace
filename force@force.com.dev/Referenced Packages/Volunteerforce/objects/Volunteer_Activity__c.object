    <validationRules>
        <fullName>Activity_Requires_Place_City</fullName>
        <active>true</active>
        <description>Volunteer Activity requires place, opportunity does not.</description>
        <errorConditionFormula>AND(
  NOT( IsOpportunity__c ),
  LEN( City__c ) = 0
)</errorConditionFormula>
        <errorDisplayField>City__c</errorDisplayField>
        <errorMessage>City is required for a Volunteer Activity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Activity_Requires_Place_Street</fullName>
        <active>true</active>
        <description>Volunteer Activity requires place, opportunity does not.</description>
        <errorConditionFormula>AND(
  NOT( IsOpportunity__c ),
  LEN( Street__c ) = 0
)</errorConditionFormula>
        <errorDisplayField>Street__c</errorDisplayField>
        <errorMessage>Street is required for a Volunteer Activity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Activity_Requires_Time</fullName>
        <active>true</active>
        <description>Volunteer Activity requires time and place, opportunity does not.</description>
        <errorConditionFormula>AND(
  NOT( IsOpportunity__c ),
  OR(
     ISNULL( Date_and_Time__c ) 
  )
)</errorConditionFormula>
        <errorDisplayField>Date_and_Time__c</errorDisplayField>
        <errorMessage>Date and Time is required for a Volunteer Activity.
If this opportunity is not associated with a date and time, please check the Ongoing Opportunity checkbox.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Duration_must_be_positive</fullName>
        <active>true</active>
        <description>Duration must be a positive number of hours</description>
        <errorConditionFormula>OR ( Duration_hours__c  &lt; 0,
    AND (
        ISNULL(Duration_hours__c ) ,
        IsOpportunity__c = false
    )
)</errorConditionFormula>
        <errorDisplayField>Duration_hours__c</errorDisplayField>
        <errorMessage>Duration must be a positive number of hours</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Duration_must_be_set</fullName>
        <active>true</active>
        <description>Duration must be set for non-ongoing opportunities</description>
        <errorConditionFormula>AND (
    ISNULL(Duration_hours__c ) ,
    IsOpportunity__c = false
)</errorConditionFormula>
        <errorDisplayField>Duration_hours__c</errorDisplayField>
        <errorMessage>Duration is required if the Ongoing Opportunity checkbox is not checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MaxParticipants</fullName>
        <active>true</active>
        <errorConditionFormula>Max_Number_of_Participants__c  &lt;  Total_Number_of_Participants__c</errorConditionFormula>
        <errorDisplayField>Max_Number_of_Participants__c</errorDisplayField>
        <errorMessage>The maximum participants value cannot be less than the number of current participants.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Organization_is_Inactive</fullName>
        <active>true</active>
        <description>Cannot add activities for organizations that are inactive.</description>
        <errorConditionFormula>AND(
ISNEW(),
ISPICKVAL(Volunteer_Organization__r.Status__c , &quot;Inactive&quot;)
)</errorConditionFormula>
        <errorMessage>Cannot add activities for organizations that are inactive.</errorMessage>
    </validationRules>
